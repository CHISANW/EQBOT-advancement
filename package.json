    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.8",
    "@types/amqplib": "^0.10.5",
    "@types/busboy": "^1.5.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@types/web3": "^1.2.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.0.0",
    "prettier-config-standard": "^7.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "reporters": [
      "default"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "./src/domains": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageDirectory": "<rootDir>/test/coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/src/main.ts",
      "/src/app.module.ts",
      "/src/version/oas-version-manage.ts",
      "/src/version/test-version-manage.ts",
      "/src/config",
      "/src/utils",
      "/src/core",
      "/src/providers",
      "/src/domains/health-check",
      "/src/repl.ts"
    ],
    "collectCoverageFrom": [
      "!src/domains/**/**/repositories/**/*.ts",
      "!src/domains/**/*.module.ts"
    ]
  }
}
