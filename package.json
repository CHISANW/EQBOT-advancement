{
  "name": "bot-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start:dev": "cross-env NODE_ENV=development TZ=UTC nest start --watch",
    "start:prod": "cross-env NODE_ENV=production TZ=UTC nest start",
    "start:test": "cross-env TZ=UTC nest start --watch",
    "make:version": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/version/oas-version-manage.ts --version",
    "docs": "npx @compodoc/compodoc -p tsconfig.json -s",
    "test": "cross-env NODE_ENV=test TZ=UTC jest",
    "test:e2e": "cross-env NODE_ENV=test TZ=UTC jest --config jest-e2e.json --forceExit --detectOpenHandles",
    "test:coverage": "cross-env NODE_ENV=test TZ=UTC jest --coverage",
    "test:report": "jest --json --outputFile=src/version/test/test-report.json; cross-env NODE_ENV=test ts-node -r tsconfig-paths/register src/version/test-version-manage.ts --version",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
